{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/*.wasm"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/_css/**",
          "/*.woff2"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "rmmapipersonal",
      "urls": ["/rest/v1/me","/ajax/from_address"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 15,
        "maxAge": "30d",
        "timeout": "5s"
      }
    },
    {
      "name": "rmmapiemails",
      "urls": ["/rest/v1/email/*"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "30d",
        "timeout": "5s"
      }
    },
    {
      "name": "folders",
      "urls": [
        "/ajax?action=ajax_getfoldercount"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 1,
        "maxAge": "30d",
        "timeout": "5s"
      }
    }
  ]
}
